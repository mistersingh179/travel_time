{
  "name": "travel-time",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "test": "jest --runInBand --verbose --watch",
    "ci-test": "jest --ci --runInBand --reporters=default --reporters=jest-junit --coverage=true",
    "badges": "jest-coverage-badges --output \"./badges\""
  },
  "dependencies": {
    "@hapi/joi": "^16.1.8",
    "cookie-parser": "~1.4.4",
    "date-holidays": "^1.4.12",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "jest-coverage-badges": "^1.1.2",
    "moment": "^2.24.0",
    "morgan": "~1.9.1",
    "weather-js": "^2.0.0"
  },
  "devDependencies": {
    "jest": "^24.9.0",
    "jest-junit": "^10.0.0",
    "supertest": "^4.0.2"
  },
  "standard": {
    "globals": [
      "test",
      "describe",
      "jest",
      "expect",
      "it",
      "beforeEach"
    ]
  },
  "jest": {
    "verbose": false,
    "displayName": "travel-time",
    "collectCoverage": false,
    "coverageReporters": [
      "json-summary",
      "text",
      "lcov"
    ]
  }
}
